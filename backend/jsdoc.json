{
  "source": {
    "include": ["./"],
    "exclude": ["./node_modules"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "./node_modules/docdash",
    "recurse": true,
    "destination": "./docs/",
    "readme": "./readme.md"
  }
}
